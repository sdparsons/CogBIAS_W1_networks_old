@article{de_jong_movement-related_2006,
	title = {Movement-related {EEG} indices of preparation in task switching and motor control},
	volume = {1105},
	issn = {00068993},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0006899306007529},
