@article{Bradley1997,
	title = {Attentional {Biases} for {Emotional} {Faces}},
	volume = {11},
	issn = {0269-9931},
	doi = {10.1080/026999397380014},
	abstract = {Examined whether the anxiety-related deployment of attention to threat words generalizes to threat pictures. 40 female undergraduates participated in Exp 1 who scored in the upper (dysphorics [DPs]) and lower (nondysphorics [NPs]) tertiles of an anxiety and depression scale. Exp 1 investigated whether there are emotion-congruent attentional biases for pictorial stimuli. In Exp 2, 9 male DPs and 13 male NPs participated. Exp 2 was the same as Exp 1, except the 2 groups of Ss were preselected to be differentiated in terms of dysphoria. Results indicate that the direction of attention to face stimuli is an interactive function of an individual's emotional state and emotional valence of the facial expression. Across both experiments, an emotion-related attentional bias was apparent when the Ss were divided on depression measures: NPs showed a greater tendency to shift attention away from threatening faces, relative to happy or neutral faces, in comparison with DPs.},
	number = {1},
	journal = {Cognition \& Emotion},
	author = {Bradley, Brendan P and Mogg, Karin and Millar, Neil and Bonham-Carter, Claire and Fergusson, Emma and Jenkins, Jane and Parr, Michelle},
	year = {1997},
