@article{price_empirical_2015,
	title = {Empirical recommendations for improving the stability of the dot-probe task in clinical research.},
	volume = {27},
	issn = {1939-134X, 1040-3590},
	url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/pas0000036},
	doi = {10.1037/pas0000036},
	abstract = {The dot-probe task has been widely used in research to produce an index of biased attention based on reaction times (RTs). Despite its popularity, very few published studies have examined psychometric properties of the task, including test-retest reliability, and no previous study has examined reliability in clinically anxious samples or systematically explored the effects of task design and analysis decisions on reliability. In the current analysis, we utilized dot-probe data from three studies where attention bias towards threat-related faces was assessed at multiple (≥5) timepoints. Two of the studies were similar (adults with Social Anxiety Disorder, similar design features) while one was much more disparate (pediatric healthy volunteers, distinct task design). We explored the effects of analysis choices (e.g., bias score calculation formula, methods for outlier handling) on reliability and searched for convergence of findings across the three studies. We found that, when considering the three studies concurrently, the most reliable RT bias index utilized data from dot-bottom trials, comparing congruent to incongruent trials, with rescaled outliers, particularly after averaging across more than one assessment point. Although reliability of RT bias indices was moderate to low under most circumstances, within-session variability in bias (attention bias variability; ABV), a recently proposed RT index, was more reliable across sessions. Several eyetracking-based indices of attention bias (available in the pediatric healthy sample only) showed reliability that matched the optimal RT index (ABV). On the basis of these findings, we make specific recommendations to researchers using the dot probe, particularly those wishing to investigate individual differences and/or single-patient applications.},
	language = {en},
	number = {2},
	urldate = {2018-10-16},
	journal = {Psychological Assessment},
	author = {Price, Rebecca B. and Kuckertz, Jennie M. and Siegle, Greg J. and Ladouceur, Cecile D. and Silk, Jennifer S. and Ryan, Neal D. and Dahl, Ronald E. and Amir, Nader},
	year = {2015},
	pages = {365--376},
	file = {Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\H623U7B2\\Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:application/pdf;Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\PAHT6XB3\\Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:application/pdf;Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\T3B89AI3\\Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:application/pdf;Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\A45YPCC3\\Price et al. - 2015 - Empirical recommendations for improving the stabil.pdf:application/pdf}
