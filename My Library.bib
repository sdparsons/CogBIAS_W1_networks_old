@article{Notebaert2014,
	title = {When we should worry more: using cognitive bias modification to drive adaptive health behaviour.},
	volume = {9},
	issn = {1932-6203},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3885669&tool=pmcentrez&rendertype=abstract},
	doi = {10.1371/journal.pone.0085092},
	abstract = {A lack of behavioural engagement in health promotion or disease prevention is a problem across many health domains. In these cases where people face a genuine danger, a reduced focus on threat and low levels of anxiety or worry are maladaptive in terms of promoting protection or prevention behaviour. Therefore, it is possible that increasing the processing of threat will increase worry and thereby enhance engagement in adaptive behaviour. Laboratory studies have shown that cognitive bias modification (CBM) can increase or decrease anxiety and worry when increased versus decreased processing of threat is encouraged. In the current study, CBM for interpretation (CBM-I) is used to target engagement in sun protection behaviour. The goal was to investigate whether inducing a negative rather than a positive interpretation bias for physical threat information can enhance worry elicited when viewing a health campaign video (warning against melanoma skin cancer), and consequently lead to more adaptive behaviour (sun protection). Participants were successfully trained to either adopt a positive or negative interpretation bias using physical threat scenarios. However, contrary to expectations results showed that participants in the positive training condition reported higher levels of worry elicited by the melanoma video than participants in the negative training condition. Video elicited worry was, however, positively correlated with a measure of engagement in sun protection behaviour, suggesting that higher levels of worry do promote adaptive behaviour. These findings imply that more research is needed to determine under which conditions increased versus decreased processing of threat can drive adaptive worry. Various potential explanations for the current findings and suggestions for future research are discussed.},
	number = {1},
	journal = {PloS one},
	author = {Notebaert, Lies and Chrystal, Jessica and Clarke, Patrick J F and Holmes, Emily a and MacLeod, Colin},
	year = {2014},
	pmid = {24416344},
	keywords = {Adolescent, Anxiety, Anxiety: psychology, Cognition, Female, Humans, Male, Young Adult, Adaptation, Health Behavior, Adaptation, Physiological, Melanoma, Melanoma: prevention \& control, Melanoma: psychology, Patient Acceptance of Health Care, Patient Acceptance of Health Care: psychology, Skin Neoplasms, Skin Neoplasms: prevention \& control, Skin Neoplasms: psychology, Sunscreening Agents, Sunscreening Agents: therapeutic use, Physiological},
	pages = {e85092},
	file = {Notebaert et al., 2014 - when we should worry more.pdf:C\:\\Users\\Sam\\Documents\\DPhil\\Notebaert et al., 2014 - when we should worry more.pdf:application/pdf}
}

@article{Harmer2013,
	title = {'{It}'s the way that you look at it'--a cognitive neuropsychological account of {SSRI} action in depression.},
	volume = {368},
	issn = {1471-2970},
	url = {http://rstb.royalsocietypublishing.org/content/368/1615/20120407.long},
	doi = {10.1098/rstb.2012.0407},
	abstract = {The fact that selective serotonin reuptake inhibitors (SSRIs) have antidepressant effects in some patients supports the notion that serotonin plays a role in the mode of action of antidepressant drugs. However, neither the way in which serotonin may alleviate depressed mood nor the reason why several weeks needs to elapse before the full antidepressant effect of treatment is expressed is known. Here, we propose a neuropsychological theory of SSRI antidepressant action based on the ability of SSRIs to produce positive biases in the processing of emotional information. Both behavioural and neuroimaging studies show that SSRI administration produces positive biases in attention, appraisal and memory from the earliest stages of treatment, well before the time that clinical improvement in mood becomes apparent. We suggest that the delay in the clinical effect of SSRIs can be explained by the time needed for this positive bias in implicit emotional processing to become apparent at a subjective, conscious level. This process is likely to involve the re-learning of emotional associations in a new, more positive emotional environment. This suggests intriguing links between the effect of SSRIs to promote synaptic plasticity and neurogenesis, and their ability to remediate negative emotional biases in depressed patients.},
	journal = {Philosophical transactions of the Royal Society of London. Series B, Biological sciences},
	author = {Harmer, Catherine J and Cowen, Philip J},
	year = {2013},
	pmid = {23440467},
	note = {ISBN: 1471-2970},
	keywords = {Anxiety, Depression, Humans, Psychological, Cognitive Therapy, Citalopram, Citalopram: administration \& dosage, Serotonin Uptake Inhibitors, Serotonin Uptake Inhibitors: administration \& dosa, Adaptation, Animals, Time Factors, Fear, Antidepressive Agents, Antidepressive Agents: pharmacology, Anxiety: drug therapy, Citalopram: pharmacology, Depression: drug therapy, Drug Synergism, Fear: drug effects, Fear: psychology, Neuropsychology, Psychological: drug effects, Serotonin Uptake Inhibitors: pharmacology, Synaptic Transmission},
	pages = {20120407},
	file = {Harmer and Cohen 2013 It’s the way that you look at it– a cognitive neuropsychological account of SSRI action in depression..pdf:C\:\\Users\\Sam\\Documents\\DPhil\\Harmer and Cohen 2013 It’s the way that you look at it– a cognitive neuropsychological account of SSRI action in depression..pdf:application/pdf}
