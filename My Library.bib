	pages = {969--77},
	file = {Beck - 2008 - The evolution of the cognitive model of depression and its neurobiological correlates.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Beck - 2008 - The evolution of the cognitive model of depression and its neurobiological correlates.pdf:application/pdf;Beck - 2008 - The Evolution of the Cognitive Model of Depression.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\7U43CCS2\\Beck - 2008 - The Evolution of the Cognitive Model of Depression.pdf:application/pdf;Beck - 2008 - The Evolution of the Cognitive Model of Depression.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\YKCST5FV\\Beck - 2008 - The Evolution of the Cognitive Model of Depression.pdf:application/pdf;Beck - 2008 - The Evolution of the Cognitive Model of Depression.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\GL9ZQM8X\\Beck - 2008 - The Evolution of the Cognitive Model of Depression.pdf:application/pdf}
