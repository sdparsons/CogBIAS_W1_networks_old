	publisher = {Guilford},
	author = {Gawronski, B and Deutsch, R and Banse, R},
	editor = {Klauer, K and Stahl, C and Voss, A},
	year = {2011},
	note = {Issue: 1},
	keywords = {*Attitude Measures, *Contextual Associations, *Implicit Measures, *Social Psychology, Attitudes, Experimentation, Measurement, Responses, reliability},
	pages = {78--123},
