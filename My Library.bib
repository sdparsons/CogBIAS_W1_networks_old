	file = {Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\DD5CVQ3P\\Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:application/pdf;Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\AY6UXU96\\Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:application/pdf;Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\8NJQMD4N\\Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:application/pdf;Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\7T56G7WT\\Lovibond and Lovibond - 1995 - The structure of negative emotional states Compar.pdf:application/pdf}
