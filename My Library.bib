	keywords = {Male, Stress, Psychological, Psychological, Stress, Stress, Psychological: physiopathology, Animals, Helplessness, Learned, Mice, Mice, Inbred C57BL, Mice, Knockout, Prefrontal Cortex, Prefrontal Cortex: pathology, Prefrontal Cortex: physiopathology, Resilience, Psychological, Stress, Psychological: pathology, Synapses, Synapses: pathology, Synapses: physiology, Resilience, Psychological: physiopathology, Helplessness, Inbred C57BL, Knockout, Learned, Psychological: pathology},
