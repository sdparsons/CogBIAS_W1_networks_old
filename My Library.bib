	pages = {433--51},
	file = {Judah et al. - 2014 - Factor structure and validation of the Attentional Control Scale.pdf:C\:\\Users\\Sam\\AppData\\Local\\Mendeley Ltd.\\Mendeley Desktop\\Downloaded\\Judah et al. - 2014 - Factor structure and validation of the Attentional Control Scale.pdf:application/pdf;Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\ZMB7NDIU\\Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:application/pdf;Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\9UGPEQ3B\\Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:application/pdf;Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\HAK4DC99\\Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:application/pdf;Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:C\:\\Users\\parsonss\\Zotero\\storage\\GI64EQMV\\Judah et al. - 2014 - Factor structure and validation of the attentional.pdf:application/pdf}
