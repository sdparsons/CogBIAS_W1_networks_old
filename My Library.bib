@article{Menne-Lothmann2014,
	title = {How to {Boost} {Positive} {Interpretations}? {A} {Meta}-{Analysis} of the {Effectiveness} of {Cognitive} {Bias} {Modification} for {Interpretation}},
	volume = {9},
	issn = {1932-6203},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4072710&tool=pmcentrez&rendertype=abstract},
	doi = {10.1371/journal.pone.0100925},
	abstract = {The current meta-analysis explores the strength of effects of cognitive{\textbackslash}nbias modification training for interpretation bias (CBM-I) on positive{\textbackslash}n(i.e., adaptive) interpretations and mood as well as the training and{\textbackslash}nsample characteristics influencing these effects. Data-bases were{\textbackslash}nsearched with the key words ``interpret\{*\} bias AND training'' and{\textbackslash}n``interpret\{*\} bias AND modif\{*\}''. Reference lists of identified{\textbackslash}narticles were checked and authors of identified articles were contacted{\textbackslash}nfor further relevant articles and unpublished data. Studies were{\textbackslash}nreviewed for inclusion with eligibility criteria being that the study{\textbackslash}n(a) aimed to target interpretation biases through any kind of training,{\textbackslash}n(b) assessed mood and/or interpretation bias as outcome measures, (c){\textbackslash}nallocated individuals to training conditions at random, and (d){\textbackslash}nrecruited adult samples. A meta-analytic multilevel mixed-effects model{\textbackslash}nwas employed to assess standardized mean changes in interpretation bias,{\textbackslash}nnegative mood, and emotional reactivity. In addition, several training{\textbackslash}nand sample characteristics were explored for their potential to enhance{\textbackslash}nbenign training effectiveness. On average, benign CBM-I resulted in an{\textbackslash}nincrease in positive interpretation bias (p{\textless}.01) and a decrease in{\textbackslash}nnegative mood state (p{\textless}.001), but did not affect emotional reactivity.{\textbackslash}nThese effects were not consistently different from control conditions{\textbackslash}nwith no or neutral training. However, within benign training conditions{\textbackslash}nimagery instructions and more training sessions were related to larger{\textbackslash}ncognitive and mood effects, whereas feedback about training performance{\textbackslash}nand inclusion of non-benign training items (instead of including benign{\textbackslash}nitems only) boosted cognitive effects only. Finally, training was more{\textbackslash}neffective in women (cognitive and mood effects) and presumably samples{\textbackslash}nwith symptomatic emotional dysregulation (cognitive effects). Although{\textbackslash}nthe effects of emotional dysregulation and number of training sessions{\textbackslash}ncould not well be distinguished, there is an indication that when used{\textbackslash}nwith imagery instructions and more training sessions, benign CBM-I can{\textbackslash}nbe employed as a useful complementary treatment to usual{\textbackslash}npsychotherapies.},
	number = {6},
	journal = {Plos One},
	author = {Menne-Lothmann, Claudia and Viechtbauer, Wolfgang and Hohn, Petra and Kasanova, Zuzana and Haller, Simone P and Drukker, Marjan and van Os, Jim and Wichers, Marieke and Lau, Jennifer Y F and Höhn, Petra and Kasanova, Zuzana and Haller, Simone P and Drukker, Marjan and van Os, Jim and Wichers, Marieke and Lau, Jennifer Y F},
	year = {2014},
	pmid = {24968234},
