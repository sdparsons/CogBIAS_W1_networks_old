	journal = {accepted for publication in BMC Medicine},
	author = {Fritz, Jessica and Stochl, Jan and Fried, Eiko I. and Goodyer, Ian and van Borkulo, Claudia and Wilkinson, Paul O. and Van Harmelen, Anne-Laura},
	year = {2019},
