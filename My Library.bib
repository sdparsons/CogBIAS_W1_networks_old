	keywords = {Humans, Stress, Psychological, Stress, Psychological: psychology, Attention, Psychological, Stress, Psychological: psychology, Stress, Psychological: physiopathology, Prefrontal Cortex, Prefrontal Cortex: physiopathology, Neuronal Plasticity, Chronic Disease, Magnetic Resonance Imaging, Psychological: physiopathology},
